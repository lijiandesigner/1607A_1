@model IEnumerable<ERP_MVC.Models.AttendanceMonth1>

@{
    ViewBag.Title = "MonthAccount";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<script src="~/Scripts/jquery-3.3.1.js"></script>
<div class="col-md-12">
    <div class="tile">
        <div class="tile-body">
            <table class="table table-hover table-bordered" id="sampleTable">
                <thead>
                    <tr>
                        <th>编号</th>
                        <th>姓名</th>
                        <th>总旷工次数</th>
                        <th>总迟到分钟数</th>
                        <th>考勤日期</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>
                        <th style="text-align:center">@item.ENo</th>
                        <th style="text-align:center">@item.EName</th>
                        <th style="text-align:center">@item.MAbsenteeism</th>
                        <th style="text-align:center">@item.MLateTime</th>
                        <th style="text-align:center">@item.Adate</th>
                    </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

